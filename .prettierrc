{
  "jsxBracketSameLine": false,
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "arrowParens": "always",
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "endOfLine": "lf",
  "tailwindFunctions": [
    "cva",
    "classNames",
    "cn"
  ],
  "tailwindAttributes": [
    "classNames"
  ]
}